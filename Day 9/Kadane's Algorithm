class Solution {
    int maxSubarraySum(int[] arr) {
        // Code here
        int n=arr.length;
        int sum=0;
        int maxsum=Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            sum+=arr[i];
            maxsum=Math.max(sum,maxsum);
            if(sum<0){
                sum=0;
            }
        }
        return maxsum;
    }
}
