// User function Template for Java

class Solution {
    public int maximumProfit(int prices[]) {
        // Code here
        int mini=prices[0];
        int profit=0;
        int maxprofit=0;
        for(int i=1;i<prices.length;i++){
            profit=prices[i]-mini;
            maxprofit=Math.max(maxprofit,profit);
            mini=Math.min(mini,prices[i]);
        }
        
        return maxprofit;
    }
}
